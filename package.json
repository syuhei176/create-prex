{
  "name": "create-prex",
  "version": "0.2.1",
  "description": "create prex app",
  "scripts": {
    "build": "bun run clean && bun run build:esm+types",
    "build:esm+types": "tsc --project tsconfig.build.json",
    "check:types": "tsc --noEmit",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "dev": "bun src/cli.ts"
  },
  "files": [
    "dist/**",
    "!dist/**/*.tsbuildinfo",
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "!src/**/*.test-d.ts",
    "templates/**"
  ],
  "bin": {
    "create-prex": "./dist/esm/cli.js"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./package.json": "./package.json"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "cross-spawn": "^7.0.3",
    "ora": "^8.1.0",
    "picocolors": "^1.1.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.6",
    "@types/node": "^20.12.10",
    "@types/prompts": "^2.4.9"
  },
  "author": "",
  "license": "ISC"
}
